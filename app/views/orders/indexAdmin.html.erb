<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">project</th>
      <th scope="col">Date creation</th>
      <th scope="col">Date Envoi</th>
      <th scope="col">Description</th>
      <th scope="col">Addresse</th>
      <th scope="col">Status</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <% @listeOrder.each do |orderTrack| %>
  <tbody>
    <tr>
      <td><%= orderTrack.id %></td>
      <td><%= orderTrack.project %></td>
      <td><%= orderTrack.dateCreated %></td>
      <td><%= orderTrack.dateSending %></td>
      <td><%= orderTrack.description %></td>
      <td><%= Addresse.find(orderTrack.addresses_id).name %></td>
      <td><%= StatusOrder.find(orderTrack.statusOrders_id).name %></td>
      <td><a href="<%= editAddviewForm_path(orderTrack.id,  orderTrack.project) %>" >edit</a></td>
    </tr>
  </tbody>
  <% end %>
</table>