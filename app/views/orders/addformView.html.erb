<% content_for :header_tags do %>
  <%= stylesheet_link_tag 'bootstrap', :plugin => 'redmine_jd_order_management' %>
  <%= javascript_include_tag 'bootstrap', :plugin => 'redmine_jd_order_management' %>

<% end %>

<h2 style="margin: 20px">Formulaire suivi</h2>
<button style="margin-bottom: 20px" type="button" class="btn btn-light">Retour</button>

<%= form_with url:putform_path(@projects), method: :post do |form| %>
  <div class="form-row">
    <div class="row">
      <%= form.label :Date_envoi, class: 'form-label' %>
      <div class="col-3">
        <%= form.date_field  :dateSending, class: 'form-control' %>
      </div>
      <div class="col-3">
        <%= form.text_field :dhl, class: 'form-control', placeholder:'NumÃ©ro DHL' %>
      </div>
    </div>
    <div style="margin-top: 15px" class="row">
      <%= form.label :description, class: 'form-label' %>
      <div class="col-12">
        <%= form.text_area(:description, size: '50x5')  %>
      </div>
      <div class="col-3">
        <%= form.text_field :status, class: 'form-control', placeholder:'Status' %>
      </div>
    </div>
    <div style="margin-top: 15px" class="row">
      <div>
        <%= form.submit "Create" %>
      </div>
    </div>

  </div>


<% end %>
